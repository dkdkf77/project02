      }
      return c ? typeof c !== "string" ? a.template(null, c) : a.template[c] || a.template(null, q.test(c) ? c : a(c)) : null
    },
    encode: function(a) {
      return ("" + a).split("<").join("&lt;").split(">").join("&gt;").split('"').join("&#34;").split("'").join("&#39;")
    }
  });
  a.extend(a.tmpl, {
    tag: {
      tmpl: {
        _default: {

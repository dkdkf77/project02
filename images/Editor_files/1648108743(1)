/**
 * 레이아웃 리스트
 */
var SDELayout = function() {
    return [];
};


/**
 * CSS 리스트
 */
var SDELayoutCSS = [
    ["/css/default.css"]
];


/**
 * Import 리스트
 */
var SDELayoutImport = [
    ["/import/menu.html"]
